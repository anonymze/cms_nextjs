generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../__db__/sqlite.db"
}

model Upload {
    id        Int     @id @default(autoincrement())
    uuid      String  @unique @default(uuid())
    filepath_public  String
    filetype  String

    @@index(fields: [uuid])
}