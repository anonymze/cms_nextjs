{
  "name": "cms_nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "develop": "npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "npx @biomejs/biome lint --apply ./src",
    "format": "npx @biomejs/biome format --write ./src",
    "db:status": "prisma migrate status --schema=./src/database/prisma/schema.prisma",
    "db:generate-global-schema": "cat ./src/database/prisma/schema/**/*.prisma > ./src/database/prisma/schema.prisma",
    "db:generate-client": "prisma generate --schema=./src/database/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./src/database/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=./src/database/prisma/schema.prisma",
    "db:admin-create": "bun run ./src/command/admin_create.js",
    "knip": "knip"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29.9",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.11.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@tanstack/react-query": "^5.24.8",
    "@tanstack/react-query-devtools": "^5.24.8",
    "@tiptap/pm": "^2.2.4",
    "@tiptap/react": "^2.2.4",
    "@tiptap/starter-kit": "^2.2.4",
    "@types/bun": "^1.0.8",
    "axios": "^1.6.7",
    "better-sqlite3": "^9.4.3",
    "bun": "^1.0.30",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.344.0",
    "negotiator": "^0.6.3",
    "next": "^14.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "sonner": "^1.4.3",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.18",
    "knip": "^5.0.3",
    "postcss": "^8.4.35",
    "postcss-nesting": "^12.0.4",
    "prisma": "^5.11.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
