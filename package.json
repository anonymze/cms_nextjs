{
  "name": "cms_nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "develop": "npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "npx @biomejs/biome lint --apply ./src",
    "format": "npx @biomejs/biome format --write ./src",
    "db:status": "prisma migrate status --schema=./src/db/prisma/schema.prisma",
    "db:generate-global-schema": "cat ./src/db/prisma/schema/**/*.prisma > ./src/db/prisma/schema.prisma",
    "db:generate-client": "prisma generate --schema=./src/db/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./src/db/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=./src/db/prisma/schema.prisma",
    "knip": "knip"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29.3",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.8.1",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@tanstack/react-query": "^5.17.12",
    "@tanstack/react-query-devtools": "^5.17.12",
    "@tiptap/pm": "^2.1.16",
    "@tiptap/react": "^2.1.16",
    "@tiptap/starter-kit": "^2.1.16",
    "axios": "^1.6.5",
    "better-sqlite3": "^9.2.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "lucide-react": "^0.307.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/node": "^20.11.4",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.4.16",
    "knip": "^4.2.1",
    "postcss": "^8.4.33",
    "prisma": "^5.8.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
